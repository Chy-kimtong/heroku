;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACb,WAAW,CAAC,UAAZ,CAAuB,EAAvB,CADa;;AAAA;AAAA;AAAA,uBAEA,WAAW,CAAC,QAAZ,EAFA;;AAAA;AAEnB,gBAAA,MAAI,CAAC,KAFc;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGpB;AARM;AAhBI,CAAf","sourcesContent":["<template>\n  <div class=\"container\">\n    <h1>Latest Tasks</h1>\n    <div class=\"create-task\">\n      <label for=\"create-task\">Say something...</label>\n      <input\n        type=\"text\"\n        id=\"create-task\"\n        v-model=\"text\"\n        placeholder=\"Create a task\"\n      />\n      <button v-on:click=\"createTask\">Create Task</button>\n    </div>\n    <hr />\n    <p class=\"error\" v-if=\"error\">{{ error }}</p>\n    <div class=\"tasks-container\">\n      <div\n        class=\"task1\"\n        v-for=\"(task, index) in tasks\"\n        v-bind:key=\"task._id\"\n        v-bind:item=\"task\"\n        v-bind:index=\"index\"\n        v-on:dblclick=\"deleteTask(task._id)\"\n      >\n        {{\n          `${task.createdAt.getDate()}/${task.createdAt.getMonth()}/${task.createdAt.getFullYear()}`\n        }}\n        <p class=\"text\">{{ task.text }}</p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport TaskService from \"../TaskService\";\nexport default {\n  name: \"TaskService\",\n  data() {\n    return {\n      tasks: [],\n      error: \"\",\n      text: \"\",\n    };\n  },\n  async created() {\n    try {\n      this.tasks = await TaskService.getTasks();\n    } catch (err) {\n      this.error = err.message;\n    }\n  },\n  methods: {\n    async createTask() {\n      await TaskService.insertTask(this.text);\n      this.tasks = await TaskService.getTasks();\n    },\n    async deleteTask(id) {\n      await TaskService.deleteTask(id);\n      this.tasks = await TaskService.getTasks();\n    },\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\ndiv.container {\n  max-width: 800px;\n  margin: 0 auto;\n}\n\np.error {\n  border: 1px solid #ff5b5f;\n  background-color: #ffc5c1;\n  padding: 10px;\n  margin-bottom: 15px;\n}\n\ndiv.task1 {\n  position: relative;\n  border: 1px solid #5bd658;\n  background-color: #bcffb8;\n  padding: 10px 10px 30px 10px;\n  margin-bottom: 15px;\n}\n\ndiv.created-at {\n  position: absolute;\n  top: 0;\n  left: 0;\n  padding: 5px 15px 5px 15px;\n  background-color: darkgreen;\n  color: white;\n  font-size: 15px;\n}\n\np.text {\n  font-size: 25px;\n  font-weight: 800;\n  margin-bottom: 0;\n}\n</style>\n"],"sourceRoot":""}]}